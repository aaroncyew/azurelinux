[mariner-update-srpms]
name=CBL-Mariner Update $releasever SRPMS
baseurl=https://packages.microsoft.com/cbl-mariner/$releasever/prod/update/srpms
gpgkey=file:///etc/pki/rpm-gpg/MICROSOFT-RPM-GPG-KEY file:///etc/pki/rpm-gpg/MICROSOFT-METADATA-GPG-KEY
gpgcheck=1
repo_gpgcheck=1
enabled=1
skip_if_unavailable=1
sslverify=1
